'use strict'

function findX() {
    // x=1;
   const x=1;// переменная созданная при пмощи const не может быть использована до обращения
}
function findY() {
     y=3;
     var y=9;
   // переменная созданная при помощи var может быть использована до обращения
   //a=1;
   let a=2;// переменная созданная при пмощи let не может быть использована до обращения
}

console.log(findX());
console.log(findY());
