'use strict'

function summ(arr) {
    for (let i=0 ; i<arr.length-1 ; i++){
      for (let j=i+1 ; j<arr.length ; j++){
        if(arr[i]>arr[j]) {
            let ind = arr[i]
            arr[i]=arr[j]
            arr[j]= ind
        }
    }  
    }
     return arr
}
console.log(summ([3,5,8,43456,2,6,9,6,3,345,67,43,12]))
